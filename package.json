{
  "name": "nuxt-pornhub-api",
  "version": "1.0.0",
  "private": true,
  "description": "My glorious Nuxt.js project",
  "author": "torish",
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "build:pro": "cross-env NODE_ENV=production nuxt build",
    "build:sta": "cross-env NODE_ENV=staging nuxt build",
    "deploy": "nuxt build && firebase deploy",
    "dev": "nuxt",
    "gcloud:gae": "nuxt build && nuxt generate && mv ~/nuxt-pornhub-api/dist/sitemap.xml ~/nuxt-pornhub-api/static/sitemap.xml && rm -rf ~/nuxt-pornhub-api/dist && gcloud app deploy && gcloud app browse",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "start": "nuxt start",
    "test": "jest --config jest.config.js"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "vue-app"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/proxy": "^2.0.1",
    "@sentry/browser": "^5.27.2",
    "@sentry/integrations": "^5.27.2",
    "@sentry/tracing": "^5.27.2",
    "@vue/test-utils": "^1.1.1",
    "axios-extensions": "^3.1.3",
    "babel-jest": "^26.6.3",
    "babel-preset-vue-app": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^7.0.0",
    "firebase": "^8.0.0",
    "firebase-tools": "^8.14.1",
    "jest": "^26.6.3",
    "nuxt": "^2.14.7",
    "nuxt-material-design-icons": "^1.0.4",
    "prettier": "^2.2.1",
    "vue-jest": "^3.0.7",
    "vue-lazyload": "^1.3.3",
    "vue-scrollto": "^2.20.0",
    "vuejs-paginate": "^2.1.0",
    "vuex-persistedstate": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@nuxtjs/eslint-config": "^4.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.4",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/tailwindcss": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-plugin-nuxt": ">=1.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "nuxt-compress": "^4.1.0"
  }
}
